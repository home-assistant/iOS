# copy this to .env and fill in the values, fastlane will pick it up

# apple id, e.g. your email address
HOMEASSISTANT_APPLE_ID=
# from the app store, e.g. Q2CBPK58CA
HOMEASSISTANT_TEAM_ID=
# numeric team id, e.g. 123456789; run `pilot list` to see it
HOMEASSISTANT_APP_STORE_CONNECT_TEAM_ID=
# lokalise auth token and project id
HOMEASSISTANT_LOKALIZE_TOKEN=
HOMEASSISTANT_LOKALIZE_PROJECT_ID=
# sentry auth token, organization and project
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=

# contents of the p8 file
APP_STORE_CONNECT_API_KEY_KEY=
# key ID
APP_STORE_CONNECT_API_KEY_KEY_ID=
# issuer ID
APP_STORE_CONNECT_API_KEY_ISSUER_ID=
